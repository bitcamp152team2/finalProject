<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.bit.admin.mapper.TeamSettingMapper">

    <select id="selectTeamUpperForOption" resultType="com.project.bit.admin.domain.TeamDTO">
    SELECT
        TEAM_UPPER
      FROM
        TEAMS
    </select>


    <select id="selectTeamForOption" resultType="com.project.bit.admin.domain.TeamDTO">
    SELECT
        TEAM_CODE
        ,TEAM_NAME
      FROM
        TEAMS
    </select>




</mapper>
